Int extern memcmp<T, K>(T* s1, K* s2, usize n);

struct Slice<T> {
	T* data;
	usize size;

	T Operator::index(self, usize index) {
	    return self.data[index];
	}
}

struct str {
	Slice<char> slice;

	usize len(self) {
		return self.slice.size;
	}

	char* data(self) {
		return self.slice.data;
	}

	bool Operator::equals(self, str string) {
	    return self.slice.size == string.slice.size
	        && !memcmp(self.slice.data, string.slice.data, self.slice.size);
	}
}

struct Range {
	usize start;
	usize end;

	Range new(usize start, usize end) {
		return Range { start, end };
	}

	usize len(self) {
		return self.end - self.start;
	}
}
